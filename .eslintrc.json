{
  "extends": ["airbnb-base"],
  "rules": {
    "prefer-const": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "comma-dangle": "off",
    "arrow-parens": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true
  }
}
